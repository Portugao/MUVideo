{# Purpose of this template: Display playlists within an external context #}
{% for playlist in items %}
    <h3>{{ playlist|muvideomodule_formattedTitle }}</h3>
    <p><a href="{{ path('muvideomodule_playlist_display', {'id': playlist.getId()}) }}" title="{{ __('Read more')|e('html_attr') }}">{{ __('Read more') }}</a>
    </p>
{% endfor %}

{# purpose of this template: module get youtuve datas page #}
{% extends 'MUVideoModule::base.html.twig' %}
{% block title __('Get playlists') %}
{% block admin_page_icon 'wrench' %}
{% block content %}
    <div class="muvideomodule-getplaylists">
        {% form_theme form with [
            '@MUVideoModule/Form/bootstrap_3.html.twig',
            'ZikulaFormExtensionBundle:Form:form_div_layout.html.twig'
        ] %}
        {{ form_start(form) }}
        <div class="zikula-bootstrap-tab-container">
        
        {{ form_row(form.channelId) }}
        {{ form_row(form.collectionId) }}
        
        <div class="form-group">
            <div class="col-sm-offset-3 col-sm-9">
                {{ form_widget(form.getDatas) }}
                {{ form_widget(form.cancel) }}
            </div>
        </div>
        {{ form_end(form) }}
    </div>
{% endblock %}
{% block footer %}
    {{ parent() }}
    {{ pageAddAsset('javascript', zasset('@MUVideoModule:js/MUVideoModule.Config.js')) }}
{% endblock %}